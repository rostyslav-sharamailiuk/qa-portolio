      "name": "Products",
      "item": [
        {
          "name": "GET All Products",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {\r",
                  "    pm.response.to.have.status(200);\r",
                  "});\r",
                  "\r",
                  "pm.test(\"Response time < 500ms\", () => {\r",
                  "    pm.expect(pm.response.responseTime).to.be.below(500);\r",
                  "});\r",
                  "\r",
                  "pm.test(\"Response has products\", function () {\r",
                  "    pm.expect(pm.response.json().length).to.be.above(0);\r",
                  "});\r",
                  "\r",
                  "const products = pm.response.json();\r",
                  "\r",
                  "pm.test(\"Response is an array\", function () {\r",
                  "    pm.expect(Array.isArray(products)).to.be.true;\r",
                  "});\r",
                  "\r",
                  "pm.test(\"Every product has required fields\", function () {\r",
                  "    products.forEach(product => {\r",
                  "        pm.expect(product.id).to.be.a('number').above(0);\r",
                  "        pm.expect(product.title).to.be.a('string').and.not.empty;\r",
                  "        pm.expect(product.price).to.be.a('number').above(0);\r",
                  "        pm.expect(product.category).to.be.a('string').and.not.empty;\r",
                  "        pm.expect(product.image).to.be.a('string').and.include('http');\r",
                  "        pm.expect(product.rating).to.be.an('object');\r",
                  "        pm.expect(product.rating.rate).to.be.a('number');\r",
                  "        pm.expect(product.rating.count).to.be.a('number');\r",
                  "    });\r",
                  "});\r",
                  "\r",
                  "pm.test(\"At least one product costs less than $20\", function () {\r",
                  "    const cheapProducts = products.filter(p => p.price < 20);\r",
                  "    pm.expect(cheapProducts.length).to.be.above(0);\r",
                  "});\r",
                  "\r",
                  "pm.test(\"All ratings are within 0 to 5\", function () {\r",
                  "    products.forEach(p => {\r",
                  "        pm.expect(p.rating.rate).to.be.within(0, 5);\r",
                  "    });\r",
                  "});"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
